    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Main Benchmark",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "ch.michel.test.MainBenchmark",
            "args": "1000 3 127.0.0.1 AKIAI6DALQXSPRIEAPWQ zerBOTixXhgqvJuij00evoJOOHYDAuYR9cYTZXYy 35.176.244.248",
            "vmArgs": "--add-modules=java.xml.bind,java.activation"
        },
        {
            "projectName": "timecrypt",
            "type": "java",
            "name": "HyperDex: Labelled Design vs Range",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "ch.michel.test.LabelledDesign",
            "args": "1000 3"
        },
        {
            "projectName": "Solitude",
            "type": "java",
            "name": "TimeCrypt: S3 Baseline vs. TimeCrypt",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "ch.michel.test.TimeCryptBenchmark",
            "args": "1000 1 AKIAI6DALQXSPRIEAPWQ zerBOTixXhgqvJuij00evoJOOHYDAuYR9cYTZXYy 127.0.0.1 8001 false",
            "vmArgs": "--add-modules=java.xml.bind,java.activation"
        },
        {
            "projectName": "Solitude",
            "type": "java",
            "name": "TimeCrypt: TimeCrypt Baseline vs. TimeCrypt",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "ch.michel.test.TimeCryptBenchmark",
            "args": "1000 1 AKIAI6DALQXSPRIEAPWQ zerBOTixXhgqvJuij00evoJOOHYDAuYR9cYTZXYy 127.0.0.1 8001 false 127.0.0.1 8002 true",
            "vmArgs": "--add-modules=java.xml.bind,java.activation"
        },
        {
            "projectName": "Solitude",
            "type": "java",
            "name": "TimeCrypt: Performance Fake Data",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "ch.michel.test.TimeCryptPerformance",
            "args": "1000 1 AKIAI6DALQXSPRIEAPWQ zerBOTixXhgqvJuij00evoJOOHYDAuYR9cYTZXYy 127.0.0.1 8001 true",
            "vmArgs": "--add-modules=java.xml.bind,java.activation"
        },
        {
            "projectName": "Solitude",
            "type": "java",
            "name": "TimeCrypt: Boundaries Benchmark",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "ch.michel.test.TimeCryptBoundariesBenchmark",
            "args": "127.0.0.1 8001 0,1,3,4,5 127.0.0.1 8002",
            "vmArgs": "--add-modules=java.xml.bind,java.activation"
        },
        {
            "projectName": "Solitude",
            "type": "java",
            "name": "Paillier vs. EC ElGamal",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "ch.michel.test.PaillierVsECElGamal"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath":"${config:python.pythonPath}",
            "program": "${file}",
            "cwd": "${workspaceFolder}/analysis/plots",
            "env": {},
            "envFile": "${workspaceFolder}/.env",
            "debugOptions": [
                "RedirectOutput"
            ]
        }
    ]
}